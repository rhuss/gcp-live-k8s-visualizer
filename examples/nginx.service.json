{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "nginx",
    "labels": {
      "visualize":"true",
      "name": "nginx",
      "app": "sample"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "http",
        "port": 9001,
        "targetPort": 80,
        "protocol": "TCP",
        "nodePort": 33333
      }
    ],
    "type": "NodePort",
    "selector": {
      "name": "nginx"
    }
  }
}
